{
  "name": "frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.js,.vue src",
    "prepare": "husky install",
    "analyze": "ANALYZE=true vite build",
    "test": "vitest",
    "test:e2e:track": "playwright test --config=playwright.config.ts",
    "test:ct": "playwright test --config=playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "3.13.8",
    "@mobily/ts-belt": "3.13.1",
    "@sentry/vite-plugin": "4.0.2",
    "@sentry/vue": "10.1.0",
    "@vue/apollo-composable": "4.2.2",
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "6.0.5",
    "neverthrow": "8.2.0",
    "pinia": "3.0.3",
    "rollup-plugin-visualizer": "6.0.3",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-toastification": "2.0.0-rc.5",
    "wavesurfer.js": "7.9.4",
    "zod": "3.25.36"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@pinia/testing": "1.0.2",
    "@playwright/experimental-ct-vue": "1.53.0",
    "@playwright/test": "1.53.0",
    "@storybook/addon-a11y": "9.0.16",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-onboarding": "9.0.16",
    "@storybook/addon-vitest": "9.0.16",
    "@storybook/vue3-vite": "9.0.16",
    "@types/node": "22.15.24",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "@vitejs/plugin-vue": "5.2.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/compiler-sfc": "3.5.17",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.28.0",
    "eslint-plugin-storybook": "9.0.16",
    "eslint-plugin-vue": "10.1.0",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.0",
    "playwright": "1.53.0",
    "prettier": "3.5.3",
    "storybook": "9.0.16",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0",
    "vite": "6.3.5",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "overrides": {
    "playwright": "1.53.0",
    "playwright-core": "1.53.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}